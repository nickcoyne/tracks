<h1><%= @track.name %> (edit)</h1>
<% if flash[:notice] %><p class="notice"><%= flash[:notice] %></p><% end -%>
<% form_tag action: 'update', id: @track do %>
  <%= render partial: 'form' %>
  <p class="form_buttons"><%= submit_tag 'Save changes' %> or <%= link_to 'Cancel', action: 'show', id: @track %></p>
<% end %>
