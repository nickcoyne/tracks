<h1>Privacy Policy (edit)</h1>
<% form_tag action: 'update', id: @special do %>
  <%= error_messages_for 'special', header_tag: 'h4' %>
  <p><%= text_area 'special', 'content', cols: 70 %></p>
  <p class="form_buttons"><%= submit_tag 'Save changes' %> or <%= link_to 'Cancel', action: 'index' %></p>
<% end %>
